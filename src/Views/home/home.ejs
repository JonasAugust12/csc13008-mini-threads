                <!-- sau 3 post đầu tiên thì sẽ xuất hiện suggestion -->
                <% if (index === 2) { %>
                    </div>
                    <div class="suggest-container px-6 py-4 border-t border-border-color">
                        <%- include('home-suggestion', {users: users}) %>
                    </div>
                    <div class="post-contents flex flex-col overflow-y-auto">
